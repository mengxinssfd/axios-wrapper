{
  "$schema": "https://typedoc.org/schema.json",
  "entryPointStrategy": "legacy-packages",
  "htmlLang": "zh-CN",
  "customCss": "./typedoc.theme.css",
  "includeVersion": true,
  "entryPoints": [
    "packages/request-template",
    "packages/axios",
    "packages/vue3-hooks",
    "packages/wechat"
  ],
  "plugin": ["@mxssfd/typedoc-theme"],
  "theme": "my-theme",
  "sort": ["static-first", "kind", "enum-value-ascending"],
  "out": "./docs-html",
  "categorizeByGroup": false,
  "searchCategoryBoosts": {
    "Component": 2,
    "Model": 1.2
  },
  "searchGroupBoosts": {
    "Classes": 1.5
  },
  "navigationLinks": {
    "GitHub": "https://github.com/mengxinssfd/request-template"
  }
}
