{
  "name": "request-template",
  "version": "1.0.0",
  "description": "请求模板封装",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "typings": "dist",
  "exports": {
    ".": {
      "import": {
        "node": "./dist/index.mjs",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/*",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "cov": "jest --coverage",
    "del-dist": "rimraf dist",
    "build:types": "tsc -p tsconfig.build.json",
    "build:esm": "tsc --build tsconfig.build.json",
    "build:cjs": "esbuild --bundle src/index.ts --format=cjs --outfile=dist/index.cjs.js --tsconfig=tsconfig.build.json",
    "build": "run-s del-dist build:cjs build:esm apie",
    "build-run": "node scripts/build.js --run --skipCheck",
    "babel": "babel dist --out-dir dist",
    "type-check": "tsc --noEmit",
    "pub": "npm publish",
    "eslint": "eslint src __test__",
    "prettier-fix": "prettier src __test__ --write",
    "prettier-check": "prettier src __test__ --check",
    "lint-check": "run-p eslint prettier-check",
    "check-all": "run-p lint-check type-check",
    "prepublish": "tsc --build tsconfig.build.json",
    "realease:old": "run-s check-all build babel changelog pub",
    "realease": "node scripts/release.js",
    "apie": "api-extractor run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/request-template.git"
  },
  "author": "DYH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mengxinssfd/request-template/issues"
  },
  "homepage": "https://github.com/mengxinssfd/request-template#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@microsoft/api-extractor": "^7.29.0",
    "@types/jest": "^29.2.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chalk": "^4.1.2",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "esbuild": "^0.14.53",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^5.1.1",
    "jest": "^29.2.1",
    "minimist": "^1.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "ts-jest": "^29.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.8.4",
    "axios": "^0.26.1"
  },
  "peerDependencies": {
    "axios": ">=0.26.1"
  }
}