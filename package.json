{
  "name": "request-template",
  "version": "1.0.0-beta.2",
  "description": "请求模板封装",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "typings": "dist",
  "sideEffects": false,
  "files": [
    "dist/*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "jest",
    "cov": "jest --coverage",
    "del-dist": "rimraf dist",
    "build:esm": "tsc --build tsconfig.build.json",
    "build:cjs": "esbuild --bundle src/index.ts --format=cjs --outfile=dist/index.cjs.js",
    "build": "run-s del-dist build:cjs build:esm apie",
    "babel": "babel dist --out-dir dist",
    "type-check": "tsc --noEmit",
    "pub": "npm publish",
    "eslint": "eslint src __test__",
    "prettier-fix": "prettier src __test__ --write",
    "prettier-check": "prettier src __test__ --check",
    "lint-check": "run-p eslint prettier-check",
    "check-all": "run-p lint-check type-check",
    "prepublish": "tsc --build tsconfig.build.json",
    "realease": "run-s check-all build babel changelog pub",
    "apie": "api-extractor run",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengxinssfd/request-template.git"
  },
  "author": "DYH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mengxinssfd/request-template/issues"
  },
  "homepage": "https://github.com/mengxinssfd/request-template#readme",
  "dependencies": {
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@microsoft/api-extractor": "^7.29.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "conventional-changelog-cli": "^2.2.2",
    "esbuild": "^0.14.53",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  }
}
